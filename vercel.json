{
     "version": 2,
     "vercel-build": [
       { "src": "package.json", "use": "@vercel/node" }
     ],
     "routes": [
       { "src": "/css/(.*)", "dest": "/public/css/" },
       { "src": "/images/(.*)", "dest": "/public/images/" },
       { "src": "/js/(.*)", "dest": "/public/js/" },
       { "src": "/(.*)", "dest": "/public/" }
     ]
   }